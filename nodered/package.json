{
    "name": "node-red-examples",
    "version": "0.0.1",
    "description": "example setup for node-RED",
    "scripts": {
        "test": "echo \"Error: no test specified\" && exit 1",
        "start": "npx node-red -v",
        "serve": "npx node-red -v --httpAdminRoot /red --httpNodeRoot /api",
        "prestart": "node $NODE_RED_HOME/node_modules/node-red-dashboard/install.js",
        "update": "npm update --unsafe-perm --no-update-notifier --no-fund",
        "rebuild": "npm rebuild --unsafe-perm --no-update-notifier --no-fund"
    },
    "author": "harunkurt.dev@gmail.com",
    "license": "Apache-2.0",
    "dependencies": {
        "node-red": "3.1.0",
        "node-red-dashboard": "3.6.1",
        "node-red-node-ui-list": "0.3.6",
        "node-red-node-ui-table": "0.4.3",
        "node-red-contrib-azure-iot-hub-send-c2d": "0.0.2",
        "node-red-contrib-azure-iot-hub": "0.4.0",
        "node-red-contrib-grpc": "1.2.5",
        "node-red-contrib-uuid": "0.0.4",
        "node-red-contrib-web-worldmap": "4.3.2",
        "@node-red-contrib-themes/theme-collection": "3.1.5"
    }
}